    "# Variables to retrieve (e.g., median household income)\n",
    "variables = ['NAME', 'B19013_001E']\n",
    "# Specify the geographic area (Los Angeles County, CA)\n",
    "for_clause = 'county subdivision:*'\n",
    "in_clause = 'state:06 county:037'\n",
